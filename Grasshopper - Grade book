function getGrade (s1, s2, s3) {
  // Create a variable to find average
	let average = (s1 + s2 + s3) / 3;
	// Use conditionals
  if (average >= 90 && average <= 100){
  return 'A';
  }else if (average >= 80){
  return 'B';
  }else if (average >= 70){
  return 'C';
  }else if(average >= 60){
  return 'D';
  }else {
  return 'F';
  }
}
